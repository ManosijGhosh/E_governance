<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Codes.LoginServlet</servlet-class>
    </servlet>
    <servlet>
	    <servlet-name>ElectionStart</servlet-name>
        <servlet-class>Codes.ElectionStart</servlet-class>
        </servlet>
    <servlet>
        <servlet-name>ElectionList</servlet-name>
        <servlet-class>Codes.ElectionList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ElectionNomination</servlet-name>
        <servlet-class>Codes.ElectionNomination</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NominationsFiled</servlet-name>
        <servlet-class>Codes.NominationsFiled</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ECCreate</servlet-name>
        <servlet-class>Codes.ECCreate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileNomination</servlet-name>
        <servlet-class>Codes.FileNominations</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
	    <servlet-name>ElectionStart</servlet-name>
	    <url-pattern>/elec_start</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ElectionList</servlet-name>
        <url-pattern>/elec_list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ElectionNomination</servlet-name>
        <url-pattern>/nom_start</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NominationsFiled</servlet-name>
        <url-pattern>/nom_filed</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ECCreate</servlet-name>
        <url-pattern>/committee</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileNomination</servlet-name>
        <url-pattern>/student_file</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        </welcome-file-list>
    </web-app>
